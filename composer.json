{
  "name": "syberisle/laravel-scribe",
  "description": "Tool for adding logging to models",
  "license": "MIT",
  "keywords": [
    "logging"
  ],
  "authors": [
    {
      "name": "David Lundgren",
      "email": "dlundgren@syberisle.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/syberisle/laravel-scribe",
  "require": {
    "php": "^8.0",
    "illuminate/contracts": "^9.28 || ^10.0 || ^11.0"
  },
  "require-dev": {
    "nunomaduro/collision": "^6.2",
    "orchestra/testbench": "^7.7 || ^8.0",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5.24",
    "squizlabs/php_codesniffer": "^3.9",
    "syberisle/coding-standards": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "SyberIsle\\Laravel\\Scribe\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SyberIsle\\Laravel\\Scribe\\Test\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "scripts": {
    "sniff": "@php vendor/bin/phpcs",
    "stan": "vendor/bin/phpstan analyse --level 6 src",
    "test": "vendor/bin/testbench package:test"
  }
}
